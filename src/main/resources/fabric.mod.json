{
  "schemaVersion": 1,
  "id": "way2wayfabric",
  "version": "${version}",
  "name": "Way2Way",
  "description": "Waystone -> Xaero's Minimap Waypoint sync for fabric",
  "authors": [
    {
      "name": "lack",
      "contact": {
        "email": "i.am@jimramsay.com",
        "homepage": "https://modrinth.com/user/lack"
      }
    }
  ],
  "contributors": [
    "ja2ui0"
  ],
  "contact": {
    "homepage": "https://github.com/lack/way2wayfabric",
    "issues": "https://github.com/lack/way2wayfabric/issues",
    "sources": "https://github.com/lack/way2wayfabric"
  },
  "license": "MIT",
  "icon": "assets/way2wayfabric/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.jimramsay.way2wayfabric.Way2WayFabric"
      }
    ]
  },
  "depends": {
    "fabricloader": ">=${fabricloader}",
    "fabric-api": ">=${fabric_api}",
    "fabric-language-kotlin": ">=${fabric_language_kotlin}",
    "minecraft": ">=${minecraft}",
    "java": ">=${java}"
  },
  "suggests": {
    "xaerominimap": ">=${xaero_minimap}",
    "xaerofairplay": ">=${xaero_fairplay}",
    "waystones": ">=${waystones}",
    "fwaystones": ">=${fwaystones}"
  }
}
